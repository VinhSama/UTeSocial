            app:popExitAnim="@anim/anim_slide_right_out" />

        <action
            android:id="@+id/action_role_candidate"
            app:destination="@id/item_fra_register_content_candidate"
            app:enterAnim="@anim/anim_slide_right_in"
            app:exitAnim="@anim/anim_slide_left_out"
            app:popEnterAnim="@anim/anim_slide_left_in"
            app:popExitAnim="@anim/anim_slide_right_out" />

    </fragment>

    <fragment
        android:id="@+id/item_fra_register_content_student"
        android:name="com.utesocial.android.feature_register.presentation.element.partial.RegisterFragmentContentStudent"
        android:label="RegisterFragmentContentStudent"
