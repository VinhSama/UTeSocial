<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_fra_register"
    app:startDestination="@id/item_fra_register_content_info">

    <fragment
        android:id="@+id/item_fra_register_content_info"
        android:name="com.utesocial.android.feature_register.presentation.element.partial.RegisterFragmentContentInfo"
        android:label="RegisterFragmentContentInfo"
        tools:layout="@layout/fragment_register_content_info">

        <action
            android:id="@+id/action_info_role"
            app:destination="@id/item_fra_register_content_role"
            app:enterAnim="@anim/anim_slide_right_in"
            app:exitAnim="@anim/anim_slide_left_out"
            app:popEnterAnim="@anim/anim_slide_left_in"
            app:popExitAnim="@anim/anim_slide_right_out" />

    </fragment>

    <fragment
        android:id="@+id/item_fra_register_content_role"
        android:name="com.utesocial.android.feature_register.presentation.element.partial.RegisterFragmentContentRole"
        android:label="RegisterFragmentContentRole"
        tools:layout="@layout/fragment_register_content_role">

        <action
            android:id="@+id/action_role_student"
            app:destination="@id/item_fra_register_content_student"
            app:enterAnim="@anim/anim_slide_right_in"
            app:exitAnim="@anim/anim_slide_left_out"
            app:popEnterAnim="@anim/anim_slide_left_in"
            app:popExitAnim="@anim/anim_slide_right_out" />

        <action
            android:id="@+id/action_role_lecturer"
            app:destination="@id/item_fra_register_content_lecturer"
            app:enterAnim="@anim/anim_slide_right_in"
